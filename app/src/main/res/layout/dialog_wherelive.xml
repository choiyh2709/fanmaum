<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <RelativeLayout
        android:id="@+id/wherelive_layout01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:background="@drawable/pop_top" >

        <TextView
            android:id="@+id/wherelive_tv01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="15dp"
            android:text="출생연도"
            android:textColor="#e62844"
            android:textSize="19dp"
            android:typeface="sans" />
    </RelativeLayout>

    <ListView
        android:id="@+id/wherelive_list01"
        android:layout_width="wrap_content"
        android:layout_height="258.5dp"
        android:layout_below="@id/wherelive_layout01"
        android:layout_centerHorizontal="true"
        android:divider="@null"
        android:listSelector="#00000000"
        android:scrollbars="none" >
    </ListView>

    <Button
        android:id="@+id/wherelive_btn01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/wherelive_list01"
        android:layout_centerHorizontal="true"
        android:background="@drawable/pop_btn03"
        android:onClick="onOK"
        android:text="@string/confirmation"
        android:textColor="#e62844"
        android:textSize="15dp" />

</RelativeLayout>