<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
   android:background="@drawable/calendar_bg"
    android:orientation="vertical" >

    
    
    <RelativeLayout android:layout_width="fill_parent" android:layout_height="58dp"
        android:id="@+id/scheduel_layout01">
        
          <ImageButton android:layout_width="30dp" android:layout_height="wrap_content"
            android:src="@drawable/calendar_prev" android:background="#00000000"
            android:layout_marginTop="24dp"  android:layout_marginLeft="15dp"
            android:id="@+id/scheduel_btn01"/>
          
        
          <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
		        android:text="10월" android:typeface="sans"
		        android:layout_marginTop="20dp" 
		        android:textSize="18dp" android:textColor="#000000"
		        android:layout_centerHorizontal="true"
		        android:id="@+id/scheduel_tv01"/>
          
           <ImageButton android:layout_width="30dp" android:layout_height="wrap_content"
            android:src="@drawable/calendar_next" android:background="#00000000"
            android:layout_marginTop="24dp"  android:layout_alignParentRight="true"
            android:layout_marginRight="15dp" 
            android:id="@+id/scheduel_btn02"/>
    
    </RelativeLayout>
    
    
    <ImageView android:layout_width="fill_parent" android:layout_height="1dp"
        android:layout_below="@id/scheduel_layout01" android:id="@+id/scheduel_img01"
        android:background="#8e8e8e"/>
           
    <LinearLayout android:layout_width="fill_parent" android:layout_height="20dp"
        android:layout_below="@id/scheduel_img01" android:orientation="horizontal"
        android:id="@+id/scheduel_layout02">
           <RelativeLayout android:layout_width="51.5dp" android:layout_height="20dp">
          <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
		        android:text="SUN" android:typeface="sans"
		        android:textSize="11dp" android:textColor="#8e8e8e"
		        android:layout_centerInParent="true"/>
          </RelativeLayout>
              <RelativeLayout android:layout_width="51.5dp" android:layout_height="20dp">
          <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
		        android:text="MON" android:typeface="sans"
		        android:textSize="11dp" android:textColor="#8e8e8e"
		        android:layout_centerInParent="true"/>
          </RelativeLayout>
              <RelativeLayout android:layout_width="51.5dp" android:layout_height="20dp">
          <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
		        android:text="TUE" android:typeface="sans"
		        android:textSize="11dp" android:textColor="#8e8e8e"
		        android:layout_centerInParent="true"/>
          </RelativeLayout>
              <RelativeLayout android:layout_width="51.5dp" android:layout_height="20dp">
          <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
		        android:text="WED" android:typeface="sans"
		        android:textSize="11dp" android:textColor="#8e8e8e"
		        android:layout_centerInParent="true"/>
          </RelativeLayout>
          <RelativeLayout android:layout_width="51.5dp" android:layout_height="20dp">
          <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
		        android:text="THU" android:typeface="sans"
		        android:textSize="11dp" android:textColor="#8e8e8e"
		        android:layout_centerInParent="true"/>
          </RelativeLayout>
          <RelativeLayout android:layout_width="51.5dp" android:layout_height="20dp">
          <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
		        android:text="FRI" android:typeface="sans"
		        android:textSize="11dp" android:textColor="#8e8e8e"
		        android:layout_centerInParent="true"/>
          </RelativeLayout>
          <RelativeLayout android:layout_width="51.5dp" android:layout_height="20dp">
          <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
		        android:text="SAT" android:typeface="sans"
		        android:textSize="11dp" android:textColor="#8e8e8e"
		        android:layout_centerInParent="true"/>
          </RelativeLayout>
    </LinearLayout>
    
       <ImageView android:layout_width="fill_parent" android:layout_height="1dp"
        android:layout_below="@id/scheduel_layout02" android:id="@+id/scheduel_img02"
        android:background="#8e8e8e"/>
    
      <android.support.v4.view.ViewPager
        android:id="@+id/scheduel_pager"
        android:layout_below="@id/scheduel_img02"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent" >
    </android.support.v4.view.ViewPager>
    
   
